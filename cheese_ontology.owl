###  http://purl.obolibrary.org/obo/FOODON_00002340
obo:FOODON_00002340 rdf:type owl:Class ;
                    rdfs:subClassOf obo:FOODON_00002403 ;
                    rdfs:comment "An inorganic, transparent, tasteless, odorless, and nearly colorless chemical substance, which is the main constituent of the fluids of all known living organisms, in which it acts as a solvent."@en ,
                                 "Una sostanza chimica inorganica, trasparente, insapore, inodore, e quasi senza colore, che è la principale costituente dei fluidi di tutti gli organismi viventi conosciuti, nei quali si comporta da solvente."@it ;
                    rdfs:label "Acqua"@it ,
                               "Water"@en ;
                    rdfs:seeAlso "https://en.wikipedia.org/wiki/Water"@en ,
                                 "https://it.wikipedia.org/wiki/Acqua"@it .


###  http://purl.obolibrary.org/obo/FOODON_00002403
obo:FOODON_00002403 rdf:type owl:Class ;
                    owl:disjointUnionOf ( obo:FOODON_00001013
                                          obo:FOODON_00001228
                                          obo:FOODON_00002221
                                          obo:FOODON_00002340
                                          obo:UBERON_0001913
                                        ) ;
                    rdfs:comment "Any substance consumed to provide nutritional support for an organism."@en ,
                                 "Ogni sostanza ingerita per fornire supporto nutrizionale ad un organismo."@it ;
                    rdfs:label "Alimento"@it ,
                               "Food"@en ;
                    rdfs:seeAlso "https://en.wikipedia.org/wiki/Food"@en ,
                                 "https://it.wikipedia.org/wiki/Alimento"@it .


###  http://purl.obolibrary.org/obo/FOODON_03301039
obo:FOODON_03301039 rdf:type owl:Class ;
                    rdfs:subClassOf :MoldRipenedCheese ;
                    rdfs:label "SoftRipenedCheese"@en .


###  http://purl.obolibrary.org/obo/FOODON_03301484
obo:FOODON_03301484 rdf:type owl:Class ;
                    rdfs:subClassOf obo:UBERON_0001913 ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasSkimming ;
