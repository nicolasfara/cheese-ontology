###  http://purl.obolibrary.org/obo/FOODON_00001125
obo:FOODON_00001125 rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( obo:FOODON_00001013
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isMadeWithMilk ;
                                                                 owl:allValuesFrom obo:FOODON_00001112
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:label "SheepCheese" .


###  http://purl.obolibrary.org/obo/FOODON_00001126
obo:FOODON_00001126 rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( obo:FOODON_00001013
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isMadeWithMilk ;
                                                                 owl:allValuesFrom obo:FOODON_00001771
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:comment "A cheese made with cow milk."@en ,
                                 "Un formaggio prodotto da latte di mucca."@it ;
                    rdfs:label "CowCheese" ,
