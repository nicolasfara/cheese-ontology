###  http://w3id.org/food/ontology/denominazione-italiana/DenominazioneOrigineControllata
food-ita:DenominazioneOrigineControllata rdf:type owl:Class ;
                                         rdfs:subClassOf food-upper:Denominazione ;
                                         rdfs:label "ControlledDesignationOrigin"@en ,
                                                    "DenominazioneOrigineControllata"@it .


###  http://w3id.org/food/ontology/denominazione-italiana/DenominazioneOrigineControllataGarantita
food-ita:DenominazioneOrigineControllataGarantita rdf:type owl:Class ;
                                                  rdfs:subClassOf food-upper:Denominazione ;
                                                  rdfs:label "ControlledGuaranteedDesignationOrigin"@en ,
                                                             "DenominazioneOrigineControllataGarantita"@it .


###  http://w3id.org/food/ontology/denominazione-italiana/IndicazioneGeograficaTipica
food-ita:IndicazioneGeograficaTipica rdf:type owl:Class ;
                                     rdfs:subClassOf food-upper:Denominazione ;
                                     rdfs:label "IndicazioneGeograficaTipica"@en ,
                                                "TypicalGeographicalIndication"@en .


###  http://w3id.org/food/ontology/denominazione-italiana/ProdottiAgroalimentariTradizionali
food-ita:ProdottiAgroalimentariTradizionali rdf:type owl:Class ;
                                            rdfs:subClassOf food-upper:Denominazione ;
                                            rdfs:label "ProdottiAgroalimentariTradizionali"@it ,
                                                       "TraditionalAgriFoodstuff"@en .


###  http://w3id.org/food/ontology/disciplinare-formaggio/DenominazioneFormaggio
food-cheese:DenominazioneFormaggio rdf:type owl:Class ;
                                   rdfs:subClassOf food-upper:Denominazione ;
                                   owl:disjointWith food-ricotta:DenominazioneRicotta ;
                                   rdfs:label "CheeseProtectedName"@en .


###  http://w3id.org/food/ontology/disciplinare-formaggio/Formaggio
food-cheese:Formaggio rdf:type owl:Class ;
                      rdfs:subClassOf food-upper:Prodotto ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty food-upper:haDenominazione ;
                                        owl:allValuesFrom food-cheese:DenominazioneFormaggio
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :isMadeWithMilk ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass :ProtectedMilkRawMaterial
                                      ] ;
                      owl:disjointWith food-ricotta:Ricotta ;
                      rdfs:label "ProtectedCheese"@en .


###  http://w3id.org/food/ontology/disciplinare-ricotta/DenominazioneRicotta
food-ricotta:DenominazioneRicotta rdf:type owl:Class ;
                                  rdfs:subClassOf food-upper:Denominazione ;
                                  rdfs:label "RicottaProtectedName"@en .


###  http://w3id.org/food/ontology/disciplinare-ricotta/Ricotta
food-ricotta:Ricotta rdf:type owl:Class ;
                     rdfs:subClassOf food-upper:Prodotto ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty food-upper:haDenominazione ;
                                       owl:allValuesFrom food-ricotta:DenominazioneRicotta
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :isMadeWithMilk ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :ProtectedMilkRawMaterial
                                     ] ;
                     rdfs:label "ProtectedRicotta"@en .


###  http://w3id.org/food/ontology/disciplinare-upper/Denominazione
food-upper:Denominazione rdf:type owl:Class ;
                         owl:disjointUnionOf ( food-ita:DenominazioneOrigineControllata
                                               food-ita:DenominazioneOrigineControllataGarantita
                                               food-ita:IndicazioneGeograficaTipica
                                               food-ita:ProdottiAgroalimentariTradizionali
                                               food-upper:DenominazioneOrigineProtetta
                                               food-upper:IndicazioneGeograficaProtetta
                                               food-upper:SpecialitaTradizionaleGarantita
                                             ) ;
                         rdfs:label "ProtectedName"@en .


###  http://w3id.org/food/ontology/disciplinare-upper/DenominazioneOrigineProtetta
food-upper:DenominazioneOrigineProtetta rdf:type owl:Class ;
                                        rdfs:subClassOf food-upper:Denominazione ;
                                        rdfs:label "DenominazioneOrigineProtetta"@it ,
                                                   "ProtectedDesignationOrigin"@en .


###  http://w3id.org/food/ontology/disciplinare-upper/IndicazioneGeograficaProtetta
food-upper:IndicazioneGeograficaProtetta rdf:type owl:Class ;
                                         rdfs:subClassOf food-upper:Denominazione ;
                                         rdfs:label "IndicazioneGeograficaProtetta"@it ,
