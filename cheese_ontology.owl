###  http://w3id.org/food/ontology/disciplinare-formaggio/DenominazioneFormaggio
food-cheese:DenominazioneFormaggio rdf:type owl:Class ;
                                   rdfs:subClassOf food-upper:Denominazione ;
                                   rdfs:label "CheeseProtectedName"@en .


###  http://w3id.org/food/ontology/disciplinare-formaggio/Formaggio
food-cheese:Formaggio rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( obo:FOODON_00001013
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty food-upper:haDenominazione ;
                                                                   owl:someValuesFrom food-cheese:DenominazioneFormaggio
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:label "ProtectedCheese"@en .


###  http://w3id.org/food/ontology/disciplinare-upper/Denominazione
food-upper:Denominazione rdf:type owl:Class ;
                         rdfs:label "ProtectedName"@en .


###  http://w3id.org/food/ontology/disciplinare-upper/MateriaPrima
food-upper:MateriaPrima rdf:type owl:Class ;
                        owl:disjointUnionOf ( agrovoc:c_1507
                                              agrovoc:c_24075
                                              agrovoc:c_33129
                                              agrovoc:c_4826
                                              agrovoc:c_8309
                                              :MilkEnzyme
                                              :Mold
                                            ) ;
                        rdfs:label "RawMaterial"@en .


###  http://w3id.org/food/ontology/disciplinare-upper/Prodotto
food-upper:Prodotto rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( obo:FOODON_00002403
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty food-upper:haDenominazione ;
                                                                 owl:someValuesFrom food-upper:Denominazione
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:label "ProtectedFood"^^xsd:string .


###  https://github.com/nicolasfara/cheese-ontology/AgedCheese
:AgedCheese rdf:type owl:Class ;
            rdfs:subClassOf obo:FOODON_00001013 ;
            owl:disjointWith :FreshCheese .


###  https://github.com/nicolasfara/cheese-ontology/Aging
:Aging rdf:type owl:Class ;
       rdfs:subClassOf :Event ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasTakenPlaceIn ;
                         owl:allValuesFrom <https://www.geonames.org/ontology#Feature>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :locatedInEnvironment ;
                         owl:allValuesFrom :Environment
                       ] ;
       owl:disjointWith :Ripeness ;
       rdfs:label "Aging"^^xsd:string .


###  https://github.com/nicolasfara/cheese-ontology/AnimalRennet
:AnimalRennet rdf:type owl:Class ;
              rdfs:subClassOf obo:FOODON_00001228 .


