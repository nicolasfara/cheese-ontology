###  http://purl.obolibrary.org/obo/FOODON_00001228
obo:FOODON_00001228 rdf:type owl:Class ;
                    rdfs:subClassOf obo:FOODON_00002403 ;
                    rdfs:comment "A complex set of enzymes which its key component is a protease enzyme that curdles the casein in milk."@en ,
                                 "Un complesso insieme di enzimi il cui componente principale è un enzima proteasi capace di addensare la caseina nel latte."@it ;
                    rdfs:label "Caglio"@it ,
                               "Rennet"@en ;
                    rdfs:seeAlso "https://en.wikipedia.org/wiki/Rennet"@en ,
                                 "https://it.wikipedia.org/wiki/Caglio"@it .


###  http://purl.obolibrary.org/obo/FOODON_00001771
obo:FOODON_00001771 rdf:type owl:Class ;
                    rdfs:subClassOf obo:UBERON_0001913 ;
                    rdfs:label "CowMilk"@en .


###  http://purl.obolibrary.org/obo/FOODON_00001911
obo:FOODON_00001911 rdf:type owl:Class ;
                    rdfs:subClassOf obo:UBERON_0001913 ;
                    rdfs:label "GoatMilk"@en .


###  http://purl.obolibrary.org/obo/FOODON_00001945
obo:FOODON_00001945 rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( obo:FOODON_00001013
