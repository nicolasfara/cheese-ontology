                    rdfs:label "CowMilk"@en ,
                               "LatteVaccino"@it ;
                    rdfs:seeAlso "https://en.wikipedia.org/wiki/Cow_milk"@en ,
                                 "https://it.wikipedia.org/wiki/Latte_vaccino"@it .


###  http://purl.obolibrary.org/obo/FOODON_00001911
obo:FOODON_00001911 rdf:type owl:Class ;
                    rdfs:subClassOf obo:UBERON_0001913 ;
                    rdfs:comment "Milk produced by milking goats."@en ,
                                 "Un latte prodotto dalla mungitura delle capre."@it ;
                    rdfs:label "GoatMilk"@en ,
                               "LatteCaprino"@it ;
                    rdfs:seeAlso "https://en.wikipedia.org/wiki/Goat_milk"@en ,
                                 "https://it.wikipedia.org/wiki/Latte_caprino"@it .


###  http://purl.obolibrary.org/obo/FOODON_00001945
obo:FOODON_00001945 rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( obo:FOODON_00001013
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :hasTexture ;
                                                                 owl:hasValue :HardTexture
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:comment "Cheese quite firmly packed into a large form and aged for months or years."@en ,
