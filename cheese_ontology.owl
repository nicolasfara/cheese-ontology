@prefix : <https://github.com/nicolasfara/cheese-ontology/> .
@prefix geo: <https://www.geonames.org/ontology#> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix agrovoc: <http://aims.fao.org/aos/agrovoc/> .
@prefix food-ita: <http://w3id.org/food/ontology/denominazione-italiana/> .
@prefix food-upper: <http://w3id.org/food/ontology/disciplinare-upper/> .
@prefix food-cheese: <http://w3id.org/food/ontology/disciplinare-formaggio/> .
@prefix food-ricotta: <http://w3id.org/food/ontology/disciplinare-ricotta/> .
@base <https://github.com/nicolasfara/cheese-ontology/> .

<https://github.com/nicolasfara/cheese-ontology/> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/food/ontology/disciplinare-upper/haDenominazione
food-upper:haDenominazione rdf:type owl:ObjectProperty ;
                           owl:inverseOf :isProtectedNameOf ;
                           rdfs:domain food-upper:Prodotto ;
                           rdfs:range food-upper:Denominazione ;
                           rdfs:label "hasProtectedName"^^xsd:string .


###  https://github.com/nicolasfara/cheese-ontology/hasAging
:hasAging rdf:type owl:ObjectProperty ;
          owl:inverseOf :isAgingOf ;
          rdf:type owl:FunctionalProperty ,
                   owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain obo:FOODON_00001013 ;
          rdfs:range :Aging .


###  https://github.com/nicolasfara/cheese-ontology/hasRipening
:hasRipening rdf:type owl:ObjectProperty ;
             owl:inverseOf :isRipeningOf ;
             rdf:type owl:FunctionalProperty ,
                      owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain obo:FOODON_00001013 ;
             rdfs:range :Ripening .


###  https://github.com/nicolasfara/cheese-ontology/hasSkimming
:hasSkimming rdf:type owl:ObjectProperty ;
             owl:inverseOf :isSkimmingOf ;
             rdf:type owl:FunctionalProperty ,
                      owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain obo:UBERON_0001913 ;
             rdfs:range :MilkSkimming .


###  https://github.com/nicolasfara/cheese-ontology/hasTakenPlaceIn
:hasTakenPlaceIn rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf geo:locatedIn ;
                 owl:inverseOf :isPlaceWhere ;
                 rdf:type owl:FunctionalProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain :Event .


###  https://github.com/nicolasfara/cheese-ontology/hasTexture
:hasTexture rdf:type owl:ObjectProperty ;
            owl:inverseOf :isTextureOf ;
            rdf:type owl:FunctionalProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain obo:FOODON_00001013 ;
            rdfs:range :CheeseTexture .


###  https://github.com/nicolasfara/cheese-ontology/isAgingOf
:isAgingOf rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isEnvironmentLocationOf
:isEnvironmentLocationOf rdf:type owl:ObjectProperty ;
                         owl:inverseOf :locatedInEnvironment .


###  https://github.com/nicolasfara/cheese-ontology/isMadeWithMilk
:isMadeWithMilk rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isMadeWithRawMaterial ;
                owl:inverseOf :isMilkUsedIn ;
                rdfs:range agrovoc:c_4826 .


###  https://github.com/nicolasfara/cheese-ontology/isMadeWithMold
:isMadeWithMold rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isMadeWithRawMaterial ;
                owl:inverseOf :isMoldUsedIn ;
                rdfs:range :Mold .


###  https://github.com/nicolasfara/cheese-ontology/isMadeWithRawMaterial
:isMadeWithRawMaterial rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isRawMaterialUsedIn ;
                       rdf:type owl:TransitiveProperty ;
                       rdfs:domain obo:FOODON_00001013 ;
                       rdfs:range food-upper:MateriaPrima .


###  https://github.com/nicolasfara/cheese-ontology/isMilkUsedIn
:isMilkUsedIn rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isMoldUsedIn
:isMoldUsedIn rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isPlaceWhere
:isPlaceWhere rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isProductionPlaceOf
:isProductionPlaceOf rdf:type owl:ObjectProperty ;
                     owl:inverseOf :producedIn .


###  https://github.com/nicolasfara/cheese-ontology/isProtectedNameOf
:isProtectedNameOf rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isRawMaterialUsedIn
:isRawMaterialUsedIn rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isRipeningOf
:isRipeningOf rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isSkimmingOf
:isSkimmingOf rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isTextureOf
:isTextureOf rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/locatedInEnvironment
:locatedInEnvironment rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain :Event ;
                      rdfs:range :Environment .


###  https://github.com/nicolasfara/cheese-ontology/producedIn
:producedIn rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf geo:locatedIn ;
            rdf:type owl:FunctionalProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain obo:FOODON_00002403 .


###  https://www.geonames.org/ontology#locatedIn
geo:locatedIn rdf:type owl:ObjectProperty ;
              rdfs:range geo:Feature .


#################################################################
#    Data properties
#################################################################

###  https://github.com/nicolasfara/cheese-ontology/hasAgingDuration
:hasAgingDuration rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Aging ;
                  rdfs:range xsd:positiveInteger ;
                  rdfs:comment "Rappresenta il tempo di stagionatura di un dato formaggio espresso in mesi"@it ,
                               "Represent the aging duration for a given cheese in months"@en .


###  https://github.com/nicolasfara/cheese-ontology/hasRipeningDuration
:hasRipeningDuration rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Ripening ;
                     rdfs:range xsd:positiveInteger ;
                     rdfs:comment "It represents the ripening time for a given cheese in days."@en ,
                                  "Rappresenta il tempo di maturazione di un dato formaggio espresso in giorni."@it .


