                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isMadeWithMilk ;
                                                                 owl:allValuesFrom obo:FOODON_00001112
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:label "SheepCheese"^^xsd:string .


###  http://purl.obolibrary.org/obo/FOODON_00001126
obo:FOODON_00001126 rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( obo:FOODON_00001013
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isMadeWithMilk ;
                                                                 owl:allValuesFrom obo:FOODON_00001771
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:label "CowCheese"^^xsd:string .


###  http://purl.obolibrary.org/obo/FOODON_00001228
obo:FOODON_00001228 rdf:type owl:Class ;
                    rdfs:subClassOf obo:FOODON_00002403 ;
