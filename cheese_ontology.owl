###  http://purl.obolibrary.org/obo/FOODON_00002231
obo:FOODON_00002231 rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( obo:FOODON_00001013
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :hasTexture ;
                                                                 owl:hasValue :SemiSoftTexture
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
