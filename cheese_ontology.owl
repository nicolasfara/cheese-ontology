@prefix : <https://github.com/nicolasfara/cheese-ontology/> .
@prefix geo: <https://www.geonames.org/ontology#> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix agrovoc: <http://aims.fao.org/aos/agrovoc/> .
@prefix food-ita: <http://w3id.org/food/ontology/denominazione-italiana/> .
@prefix food-upper: <http://w3id.org/food/ontology/disciplinare-upper/> .
@prefix food-cheese: <http://w3id.org/food/ontology/disciplinare-formaggio/> .
@prefix food-ricotta: <http://w3id.org/food/ontology/disciplinare-ricotta/> .
@base <https://github.com/nicolasfara/cheese-ontology/> .

<https://github.com/nicolasfara/cheese-ontology/> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/food/ontology/disciplinare-upper/haDenominazione
food-upper:haDenominazione rdf:type owl:ObjectProperty ;
                           owl:inverseOf :isProtectedNameOf ;
                           rdfs:domain food-upper:Prodotto ;
                           rdfs:range food-upper:Denominazione ;
                           rdfs:label "hasProtectedName"^^xsd:string .


###  https://github.com/nicolasfara/cheese-ontology/hasAging
:hasAging rdf:type owl:ObjectProperty ;
          owl:inverseOf :isAgingOf ;
          rdf:type owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain :AgedCheese ;
          rdfs:range :Aging .


###  https://github.com/nicolasfara/cheese-ontology/hasRipening
:hasRipening rdf:type owl:ObjectProperty ;
             owl:inverseOf :isRipeningOf ;
             rdf:type owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain obo:FOODON_00001013 ;
             rdfs:range :Ripening .


###  https://github.com/nicolasfara/cheese-ontology/hasSkimming
:hasSkimming rdf:type owl:ObjectProperty ;
             owl:inverseOf :isSkimmingOf ;
             rdf:type owl:FunctionalProperty ,
                      owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain obo:UBERON_0001913 ;
             rdfs:range :MilkSkimming .


###  https://github.com/nicolasfara/cheese-ontology/hasTakenPlaceIn
:hasTakenPlaceIn rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :locatedIn ;
                 owl:inverseOf :isPlaceWhere ;
                 rdf:type owl:FunctionalProperty ,
                          owl:AsymmetricProperty ,
                          owl:IrreflexiveProperty ;
                 rdfs:domain :Event .


###  https://github.com/nicolasfara/cheese-ontology/hasTexture
:hasTexture rdf:type owl:ObjectProperty ;
            owl:inverseOf :isTextureOf ;
            rdf:type owl:FunctionalProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain obo:FOODON_00001013 ;
            rdfs:range :Texture .


###  https://github.com/nicolasfara/cheese-ontology/isAgingOf
:isAgingOf rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isEnvironmentLocationOf
:isEnvironmentLocationOf rdf:type owl:ObjectProperty ;
                         owl:inverseOf :locatedInEnvironment .


###  https://github.com/nicolasfara/cheese-ontology/isMadeWithMilk
:isMadeWithMilk rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isMadeWithRawMaterial ;
                owl:inverseOf :isMilkUsedIn ;
                rdfs:range agrovoc:c_4826 .


###  https://github.com/nicolasfara/cheese-ontology/isMadeWithMold
:isMadeWithMold rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isMadeWithRawMaterial ;
                owl:inverseOf :isMoldUsedIn ;
                rdfs:domain :MoldRipenedCheese ;
                rdfs:range :Mold .


###  https://github.com/nicolasfara/cheese-ontology/isMadeWithRawMaterial
:isMadeWithRawMaterial rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isRawMaterialUsedIn ;
                       rdf:type owl:TransitiveProperty ;
                       rdfs:domain obo:FOODON_00001013 ;
                       rdfs:range food-upper:MateriaPrima .


###  https://github.com/nicolasfara/cheese-ontology/isMilkUsedIn
:isMilkUsedIn rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isMoldUsedIn
:isMoldUsedIn rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isPlaceWhere
:isPlaceWhere rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isProductionPlaceOf
:isProductionPlaceOf rdf:type owl:ObjectProperty ;
                     owl:inverseOf :producedIn .


###  https://github.com/nicolasfara/cheese-ontology/isProtectedNameOf
:isProtectedNameOf rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isRawMaterialUsedIn
:isRawMaterialUsedIn rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isRipeningOf
:isRipeningOf rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isSkimmingOf
:isSkimmingOf rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/isTextureOf
:isTextureOf rdf:type owl:ObjectProperty .


###  https://github.com/nicolasfara/cheese-ontology/locatedIn
:locatedIn rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ,
                    owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty ;
           rdfs:range <http://dbpedia.org/ontology/PopulatedPlace> .


###  https://github.com/nicolasfara/cheese-ontology/locatedInEnvironment
:locatedInEnvironment rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain :Event ;
                      rdfs:range :Environment .


###  https://github.com/nicolasfara/cheese-ontology/producedIn
:producedIn rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :locatedIn ;
            rdf:type owl:FunctionalProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain obo:FOODON_00002403 .


#################################################################
#    Data properties
#################################################################

###  https://github.com/nicolasfara/cheese-ontology/hasAgingDuration
:hasAgingDuration rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Aging ;
                  rdfs:range xsd:positiveInteger ;
                  rdfs:comment "Rappresenta il tempo di stagionatura di un dato formaggio espresso in mesi"@it ,
                               "Represent the aging duration for a given cheese in months"@en .


###  https://github.com/nicolasfara/cheese-ontology/hasRipeningDuration
:hasRipeningDuration rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Ripening ;
                     rdfs:range xsd:positiveInteger ;
                     rdfs:comment "It represents the ripening time for a given cheese in days."@en ,
                                  "Rappresenta il tempo di maturazione di un dato formaggio espresso in giorni."@it .


#################################################################
#    Classes
#################################################################

###  http://aims.fao.org/aos/agrovoc/c_1507
agrovoc:c_1507 rdf:type owl:Class ;
               rdfs:subClassOf obo:FOODON_00001013 ,
                               food-upper:MateriaPrima ;
               rdfs:comment "A cheese used as a raw material in the production of another cheese."@en ,
                            "Un formaggio usato come materia prima nella produzione di un altro formaggio."@it ;
               rdfs:label "CheeseRawMaterial"@en ,
                          "MateriaPrimaFormaggio"@it .


###  http://aims.fao.org/aos/agrovoc/c_24075
agrovoc:c_24075 rdf:type owl:Class ;
                rdfs:subClassOf obo:FOODON_00001228 ,
                                food-upper:MateriaPrima ;
                rdfs:comment "A complex set of enzymes which its key component is a protease enzyme that curdles the casein in milk and is then used as a raw material in the cheese production process."@en ,
                             "Un complesso insieme di enzimi il cui componente principale è un enzima proteasi capace di addensare la caseina nel latte ed è quindi usato come materia prima nel processo produttivo del formaggio."@it ;
                rdfs:label "MateriaPrimaCaglio"@it ,
                           "RennetRawMaterial"@en .


###  http://aims.fao.org/aos/agrovoc/c_33129
agrovoc:c_33129 rdf:type owl:Class ;
                rdfs:subClassOf obo:FOODON_00002221 ,
                                food-upper:MateriaPrima ;
                rdfs:comment "Un minerale composto principalmente di cloruro di sodio utilizzato come materia prima nella produzione del formaggio dove ha sia lo scopo di conservante che di insaporitore."@it ;
                rdfs:label "MateriaPrimaSale"@it ,
                           "SaltRawMaterial"@en ;
                rdfs:seeAlso "A mineral composed primarily of sodium chloride used as a raw material in the cheese production process where it functions as both a preservative and a flavoring."@en ,
                             "https://en.wikipedia.org/wiki/Salt#Salt_in_food"@en ,
                             "https://it.wikipedia.org/wiki/Sale_da_cucina#Usi"@it .


###  http://aims.fao.org/aos/agrovoc/c_4826
agrovoc:c_4826 rdf:type owl:Class ;
               rdfs:subClassOf obo:UBERON_0001913 ,
                               food-upper:MateriaPrima ;
               rdfs:comment "A milk used as a raw material in the cheese production process."@en ,
                            "Un latte usato come materia prima nella produzione di un formaggio."@it ;
               rdfs:label "MateriaPrimaLatte"@it ,
                          "MilkRawMaterial"@en .


###  http://aims.fao.org/aos/agrovoc/c_8309
agrovoc:c_8309 rdf:type owl:Class ;
               rdfs:subClassOf obo:FOODON_00002340 ,
                               food-upper:MateriaPrima ;
               rdfs:comment "A kind of water when used as a raw material in the cheese production process."@en ,
                            "Un'acqua quando utilizzata come materia prima nel processo di produzione del formaggio."@it ;
               rdfs:label "WaterRawMaterial"@en ;
               rdfs:seeAlso "MateriaPrimaAcqua"@it .


###  http://aims.fao.org/aos/agrovoc/c_8376
agrovoc:c_8376 rdf:type owl:Class ;
               rdfs:subClassOf food-upper:MateriaPrima ;
               rdfs:comment "Il siero di latte è la parte liquida del latte che si separa dalla cagliata durante la caseificazione."@it ,
                            "Whey is the liquid remaining after milk has been curdled and strained."@en ;
               rdfs:label "Siero"@it ,
                          "Whey"@en ;
               rdfs:seeAlso "https://en.wikipedia.org/wiki/Whey"@en ,
                            "https://it.wikipedia.org/wiki/Siero_di_latte"@it .


###  http://dbpedia.org/ontology/PopulatedPlace
<http://dbpedia.org/ontology/PopulatedPlace> rdf:type owl:Class ;
                                             rdfs:comment "A place or area with clustered or scattered buildings and a permanent human population referenced with geographic coordinates."@en ,
                                                          "Un luogo o un area dotata di edifici raggruppati o sparsi e una popolazione umana a cui è possibile fare riferimento mediante coordinate geografiche."@it ;
                                             rdfs:label "InsediamentoUmano"@it ,
                                                        "PopulatedPlace"@en ;
                                             rdfs:seeAlso "http://en.wikipedia.org/wiki/Populated_place"@en ,
                                                          "https://it.wikipedia.org/wiki/Insediamento_umano"@it .


###  http://purl.obolibrary.org/obo/ENVO_00000067
obo:ENVO_00000067 rdf:type owl:Class ;
                  rdfs:subClassOf :Environment ;
                  rdfs:comment "A large hole in the side of a hill, cliff, or mountain, or one that is underground, used for cheese affinage."@en ,
                               "Cavità naturale a sviluppo prevalentemente orizzontale che si addentra nei fianchi di un monte, di una collina, di una scarpata o nel sottosuolo, utilizzata per l'affinatura dei formaggi."@it ;
                  rdfs:label "Cave"@en ,
                             "Grotta"@it .


###  http://purl.obolibrary.org/obo/ENVO_01000583
obo:ENVO_01000583 rdf:type owl:Class ;
                  rdfs:subClassOf :Environment ;
                  rdfs:comment "A large piece of equipment that uses electricity to preserve cheese at a cold temperature."@en ,
                               "CellaFrigorifera"@it ,
                               "Un locale di conservazione del formaggio, ove viene mantenuta una temperatura inferiore a quella ambientale."@it ;
                  rdfs:label "Refrigerator"@en .


###  http://purl.obolibrary.org/obo/ENVO_01001871
obo:ENVO_01001871 rdf:type owl:Class ;
                  rdfs:subClassOf :Environment ;
                  rdfs:comment "A large hole in the ground, varying in depth, used for cheese affinage."@en ,
                               "Scavatura del terreno, più o meno profonda, praticata per l'affinatura dei formaggi."@it ;
                  rdfs:label "Fossa"@it ,
                             "Pit"@en .


###  http://purl.obolibrary.org/obo/FOODON_00001013
obo:FOODON_00001013 rdf:type owl:Class ;
                    rdfs:subClassOf obo:FOODON_00002403 ;
                    owl:disjointUnionOf ( obo:FOODON_00001124
                                          obo:FOODON_00001125
                                          obo:FOODON_00001126
                                          :BuffaloCheese
                                          :MixedMilkCheese
                                        ) ,
                                        ( obo:FOODON_00001945
                                          obo:FOODON_00002230
                                          obo:FOODON_00002231
                                          obo:FOODON_00002252
                                          :MediumHardCheese
                                        ) ,
                                        ( obo:FOODON_03401217
                                          :SkimmedMilkCheese
                                          :WholeMilkCheese
                                        ) ,
                                        ( :AgedCheese
                                          :FreshCheese
                                        ) ;
                    rdfs:comment "Cheese is a dairy product produced in wide ranges of flavors, textures, and forms by coagulation of the milk protein casein."@en ,
                                 "Il formaggio è un prodotto caseario ottenuto dalla coagulazione acida o presamica del latte intero, parzialmente o totalmente scremato per coagulazione delle caseine, facendo anche uso di fermenti e sale da cucina."@it ;
                    rdfs:label "Cheese"@en ,
                               "Formaggio"@it ;
                    rdfs:seeAlso "https://en.wikipedia.org/wiki/Cheese"@en ,
                                 "https://it.wikipedia.org/wiki/Formaggio"@it .


###  http://purl.obolibrary.org/obo/FOODON_00001065
obo:FOODON_00001065 rdf:type owl:Class ;
                    rdfs:subClassOf :MoldRipenedCheese ;
                    rdfs:label "BlueCheese"@en .


###  http://purl.obolibrary.org/obo/FOODON_00001108
obo:FOODON_00001108 rdf:type owl:Class ;
                    rdfs:subClassOf obo:UBERON_0001913 ;
                    rdfs:label "BuffaloMilk"@en .


###  http://purl.obolibrary.org/obo/FOODON_00001112
obo:FOODON_00001112 rdf:type owl:Class ;
                    rdfs:subClassOf obo:UBERON_0001913 ;
                    rdfs:label "SheepMilk"@en .


###  http://purl.obolibrary.org/obo/FOODON_00001124
obo:FOODON_00001124 rdf:type owl:Class ;
