@prefix : <https://github.com/nicolasfara/cheese-ontology/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix agrovoc: <http://aims.fao.org/aos/agrovoc/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix food-ita: <http://w3id.org/food/ontology/denominazione-italiana/> .
@prefix food-upper: <http://w3id.org/food/ontology/disciplinare-upper/> .
@prefix food-cheese: <http://w3id.org/food/ontology/disciplinare-formaggio/> .
@prefix food-ricotta: <http://w3id.org/food/ontology/disciplinare-ricotta/> .
@base <https://github.com/nicolasfara/cheese-ontology/> .

<https://github.com/nicolasfara/cheese-ontology/> rdf:type owl:Ontology ;
                                                   dcterms:author "Linda Vitali" ,
                                                                  "Matteo Castellucci" ,
                                                                  "Nicolas Farabegoli" ;
                                                   dcterms:license "https://opensource.org/licenses/MIT"@en ;
                                                   dcterms:title "Cheese Ontology"@en ;
                                                   vann:preferredNamespacePrefix "cheese" ;
                                                   vann:preferredNamespaceUri "https://github.com/nicolasfara/cheese-ontology/" ;
                                                   rdfs:comment "An ontology modelling cheese, its production process, the raw materials involved and the certifications it can own."@en ;
                                                   owl:versionInfo "1.0.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/author
dcterms:author rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://w3id.org/food/ontology/disciplinare-upper/haDenominazione
food-upper:haDenominazione rdf:type owl:ObjectProperty ;
                           owl:inverseOf :isProtectedNameOf ;
                           rdfs:domain food-upper:Prodotto ;
                           rdfs:range food-upper:Denominazione ;
                           rdfs:comment "Associa un alimento certificato alla sua denominazione."@it ,
                                        "Relates a protected food to its protected name."@en ;
                           rdfs:label "haDenominazione"@it ,
                                      "hasProtectedName"@en .


###  https://github.com/nicolasfara/cheese-ontology/hasAging
:hasAging rdf:type owl:ObjectProperty ;
          owl:inverseOf :isAgingOf ;
          rdf:type owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain :AgedCheese ;
          rdfs:range :Aging ;
          rdfs:comment "Associa un formaggio stagionato alla sua stagionatura."@it ,
                       "Relates an aged cheese with its aging."@en ;
          rdfs:label "haStagionatura"@it ,
                     "hasAging"@en .


###  https://github.com/nicolasfara/cheese-ontology/hasRipening
:hasRipening rdf:type owl:ObjectProperty ;
             owl:inverseOf :isRipeningOf ;
             rdf:type owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain obo:FOODON_00001013 ;
             rdfs:range :Ripening ;
             rdfs:comment "Associa un formaggio alla sua maturazione."@it ,
                          "Relates a cheese to its ripening."@en ;
             rdfs:label "haMaturazione"@it ,
                        "hasRipening"@en .


###  https://github.com/nicolasfara/cheese-ontology/hasSkimming
:hasSkimming rdf:type owl:ObjectProperty ;
             owl:inverseOf :isSkimmingOf ;
             rdf:type owl:FunctionalProperty ,
                      owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:domain obo:UBERON_0001913 ;
             rdfs:range :MilkSkimming ;
             rdfs:comment "Associa un latte alla sua tipologia di scrematura."@it ,
                          "Relates a milk to its skimming."@en ;
             rdfs:label "haScrematura"@it ,
                        "hasSkimming"@en .


###  https://github.com/nicolasfara/cheese-ontology/hasTakenPlaceIn
:hasTakenPlaceIn rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :isLocatedIn ;
                 owl:inverseOf :isPlaceWhere ;
                 rdfs:domain :Event ;
                 rdfs:comment "Associa un evento all'insediamento umano più vicino al luogo in cui quell'evento si svolge."@it ,
                              "Relates an event to the populated place nearest to the place in which that event takes place."@en ;
                 rdfs:label "hasTakenPlaceIn"@en ,
                            "siSvolgeA"@it .


###  https://github.com/nicolasfara/cheese-ontology/hasTexture
:hasTexture rdf:type owl:ObjectProperty ;
            owl:inverseOf :isTextureOf ;
            rdf:type owl:FunctionalProperty ,
                     owl:AsymmetricProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain obo:FOODON_00001013 ;
            rdfs:range :Texture ;
            rdfs:comment "Associa un formaggio al suo tipo di pasta."@it ,
                         "Relates a cheese to its texture."@en ;
            rdfs:label "haPasta"@it ,
                       "hasTexture"@en .


###  https://github.com/nicolasfara/cheese-ontology/isAgingOf
:isAgingOf rdf:type owl:ObjectProperty ;
           rdfs:comment "Associa una stagionatura al formaggio stagionato che l'ha subita."@it ,
                        "Relates an aging to the aged cheese that endured it."@en ;
           rdfs:label "isAgingOf"@en ,
                      "èStagionaturaDi"@it .


###  https://github.com/nicolasfara/cheese-ontology/isEnvironmentLocationOf
:isEnvironmentLocationOf rdf:type owl:ObjectProperty ;
                         owl:inverseOf :isLocatedInEnvironment ;
                         rdfs:comment "Associa un ambiente con un evento che è avvenuto in quell'ambiente."@it ,
                                      "Relates an environment with an event that took place in that environment."@en ;
                         rdfs:label "isEnvironmentLocationOf"@en ,
                                    "èLuogoAmbientaleDi"@it .


###  https://github.com/nicolasfara/cheese-ontology/isLocatedIn
:isLocatedIn rdf:type owl:ObjectProperty ;
             owl:inverseOf :isLocationOf ;
             rdf:type owl:FunctionalProperty ,
                      owl:AsymmetricProperty ,
                      owl:IrreflexiveProperty ;
             rdfs:range dbo:PopulatedPlace ;
             rdfs:comment "Associa un qualcosa con l'insediamento umano più vicino al luogo in cui quel qualcosa si è svolto."@it ,
                          "Relates something with the populated place nearest to the place in which that something happened."@en ;
             rdfs:label "isLocatedIn"@en ,
                        "èLocalizzatoA"@it .


###  https://github.com/nicolasfara/cheese-ontology/isLocatedInEnvironment
:isLocatedInEnvironment rdf:type owl:ObjectProperty ,
                                 owl:FunctionalProperty ,
                                 owl:AsymmetricProperty ,
                                 owl:IrreflexiveProperty ;
                        rdfs:domain :Event ;
                        rdfs:range :Environment ;
                        rdfs:comment "Associa un evento all'ambiente in cui si è svolto."@it ,
                                     "Relates an event to the environment in which it happened."@en ;
                        rdfs:label "isLocatedInEnvironment"@en ,
                                   "èLocalizzatoInAmbiente"@it .


###  https://github.com/nicolasfara/cheese-ontology/isLocationOf
:isLocationOf rdf:type owl:ObjectProperty ;
              rdfs:comment "Associa l'insediamento umano più vicino al luogo in cui qualcosa si è svolto con questo qualcosa."@it ,
                           "Relates the populated place nearest to a place in which something happened to this something."@en ;
              rdfs:label "isLocationOf"@en ,
                         "èLuogoDi"@it .


###  https://github.com/nicolasfara/cheese-ontology/isMadeWithMilk
:isMadeWithMilk rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isMadeWithRawMaterial ;
                owl:inverseOf :isMilkUsedIn ;
                rdfs:range agrovoc:c_4826 ;
                rdfs:comment "Associa un formaggio al latte da cui è fatto."@it ,
                             "Relates a cheese to the milk used for producing it."@en ;
                rdfs:label "isMadeWithMilk"@en ,
                           "èFattoDaLatte"@it .


###  https://github.com/nicolasfara/cheese-ontology/isMadeWithMold
:isMadeWithMold rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isMadeWithRawMaterial ;
                owl:inverseOf :isMoldUsedIn ;
                rdfs:domain :MoldRipenedCheese ;
                rdfs:range :Mold ;
                rdfs:comment "Associa un formaggio ammuffito con una muffa di cui è fatto."@it ,
                             "Relates a mold-ripened cheese to a mold used for producing it."@en ;
                rdfs:label "isMadeWithMold"@en ,
                           "èFattoDaMuffa"@it .


###  https://github.com/nicolasfara/cheese-ontology/isMadeWithRawMaterial
:isMadeWithRawMaterial rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isRawMaterialUsedIn ;
                       rdf:type owl:TransitiveProperty ;
                       rdfs:domain obo:FOODON_00001013 ;
                       rdfs:range food-upper:MateriaPrima ;
                       rdfs:comment "Associa un formaggio ad una materia prima di cui è fatto."@it ,
                                    "Relates a cheese to a raw material used for producing it."@en ;
                       rdfs:label "isMadeWithRawMaterial"@en ,
                                  "èFattoDaMateriaPrima"@it .


###  https://github.com/nicolasfara/cheese-ontology/isMilkUsedIn
:isMilkUsedIn rdf:type owl:ObjectProperty ;
              rdfs:comment "Associa un latte al formaggio in cui è stato utilizzato."@it ,
                           "Relates a milk to a cheese in which it has been used."@en ;
              rdfs:label "isMilkUsedIn"@en ,
                         "èLatteUsatoIn"@it .


###  https://github.com/nicolasfara/cheese-ontology/isMoldUsedIn
:isMoldUsedIn rdf:type owl:ObjectProperty ;
              rdfs:comment "Associa una muffa ad un formaggio ammuffito in cui è usata."@it ,
                           "Relates a mold with a mold-ripened cheese in which it has been used."@en ;
              rdfs:label "isMoldUsedIn"@en ,
                         "èMuffaUsataIn"@it .


###  https://github.com/nicolasfara/cheese-ontology/isPlaceWhere
:isPlaceWhere rdf:type owl:ObjectProperty ;
              rdfs:comment "Associa l'insediamento umano più vicino al luogo in cui i è svolto un evento con l'evento stesso."@it ,
                           "Relates the populated place nearest to the place in which an event happened to the event itself."@en ;
              rdfs:label "isPlaceWhere"@en ,
                         "èLuogoInCui"@it .


###  https://github.com/nicolasfara/cheese-ontology/isProducedIn
:isProducedIn rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isLocatedIn ;
              owl:inverseOf :isProductionPlaceOf ;
              rdfs:domain obo:FOODON_00002403 ;
              rdfs:comment "Associa un alimento all'insediamento umano più vicino al luogo in cui è stato prodotto."@it ,
                           "Relates a food to the populated place nearest to the place in which was produced."@en ;
              rdfs:label "isProducedIn"@en ,
                         "èProdottoA"@it .


###  https://github.com/nicolasfara/cheese-ontology/isProductionPlaceOf
:isProductionPlaceOf rdf:type owl:ObjectProperty ;
                     rdfs:comment "Associa l'insediamento umano più vicino al luogo di produzione di un alimento all'alimento stesso."@it ,
                                  "Relates the populated place nearest to the production place of a food to the food itself."@en ;
                     rdfs:label "isProductionPlaceOf"@en ,
                                "èLuogoDiProduzioneDi"@it .


###  https://github.com/nicolasfara/cheese-ontology/isProtectedNameOf
:isProtectedNameOf rdf:type owl:ObjectProperty ;
                   rdfs:comment "Associa una denominazione ad un alimento certificato che la possiede."@it ,
                                "Relates a certified name to a certified food that owns it."@en ;
                   rdfs:label "isProtectedNameOf"@en ,
                              "èDenominazioneDi"@it .


###  https://github.com/nicolasfara/cheese-ontology/isRawMaterialUsedIn
:isRawMaterialUsedIn rdf:type owl:ObjectProperty ;
                     rdfs:comment "Associa una materia prima ad un formaggio in cui è stata utilizzata."@it ,
                                  "Relates a raw material to a cheese in which it has been used."@en ;
                     rdfs:label "isRawMaterialUsedIn"@en ,
                                "èMateriaPrimaUsataIn"@it .


###  https://github.com/nicolasfara/cheese-ontology/isRipeningOf
:isRipeningOf rdf:type owl:ObjectProperty ;
              rdfs:comment "Associa una maturazione ad un formaggio che la subisce."@it ,
                           "Relates a ripening to a cheese that endures it."@en ;
              rdfs:label "isRipeningOf"@en ,
                         "èMaturazioneDi"@it .


###  https://github.com/nicolasfara/cheese-ontology/isSkimmingOf
:isSkimmingOf rdf:type owl:ObjectProperty ;
              rdfs:comment "Associa una tipologia di scrematura al latte che la subisce."@it ,
                           "Relates a skimming procedure to a milk that endures it."@en ;
              rdfs:label "isSkimmingOf"@en ,
                         "èScrematuraDi"@it .


###  https://github.com/nicolasfara/cheese-ontology/isTextureOf
:isTextureOf rdf:type owl:ObjectProperty ;
             rdfs:comment "Associa una tipologia di pasta con un formaggio che la possiede."@it ,
                          "Relates a texture to a cheese that has it."@en ;
             rdfs:label "isTextureOf"@en ,
                        "èPastaDi"@it .


#################################################################
#    Data properties
#################################################################

###  https://github.com/nicolasfara/cheese-ontology/hasAgingDuration
:hasAgingDuration rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :Aging ;
                  rdfs:range xsd:positiveInteger ;
                  rdfs:comment "Rappresenta il tempo di stagionatura di un dato formaggio espresso in mesi."@it ,
                               "Represents the aging duration for a given cheese in months."@en ;
                  rdfs:label "haDurataStagionatura"@it ,
                             "hasAgingDuration"@en .


###  https://github.com/nicolasfara/cheese-ontology/hasRipeningDuration
:hasRipeningDuration rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :Ripening ;
                     rdfs:range xsd:positiveInteger ;
                     rdfs:comment "Rappresenta il tempo di maturazione di un dato formaggio espresso in giorni."@it ,
                                  "Represents the ripening time for a given cheese in days."@en ;
                     rdfs:label "haDurataMaturazione"@it ,
                                "hasRipeningDuration"@en .


#################################################################
#    Classes
#################################################################

###  http://aims.fao.org/aos/agrovoc/c_1507
agrovoc:c_1507 rdf:type owl:Class ;
               rdfs:subClassOf obo:FOODON_00001013 ,
                               food-upper:MateriaPrima ;
               rdfs:comment "A cheese used as a raw material in the production of another cheese."@en ,
                            "Un formaggio usato come materia prima nella produzione di un altro formaggio."@it ;
               rdfs:label "CheeseRawMaterial"@en ,
                          "MateriaPrimaFormaggio"@it .


###  http://aims.fao.org/aos/agrovoc/c_24075
agrovoc:c_24075 rdf:type owl:Class ;
                rdfs:subClassOf obo:FOODON_00001228 ,
                                food-upper:MateriaPrima ;
                rdfs:comment "A complex set of enzymes which its key component is a protease enzyme that curdles the casein in milk and is then used as a raw material in the cheese production process."@en ,
                             "Un complesso insieme di enzimi il cui componente principale è un enzima proteasi capace di addensare la caseina nel latte ed è quindi usato come materia prima nel processo produttivo del formaggio."@it ;
                rdfs:label "MateriaPrimaCaglio"@it ,
                           "RennetRawMaterial"@en .


###  http://aims.fao.org/aos/agrovoc/c_33129
agrovoc:c_33129 rdf:type owl:Class ;
                rdfs:subClassOf obo:FOODON_00002221 ,
                                food-upper:MateriaPrima ;
                rdfs:comment "A mineral composed primarily of sodium chloride used as a raw material in the cheese production process where it functions as both a preservative and a flavoring."@en ,
                             "Un minerale composto principalmente di cloruro di sodio utilizzato come materia prima nella produzione del formaggio dove ha sia lo scopo di conservante che di insaporitore."@it ;
                rdfs:label "MateriaPrimaSale"@it ,
                           "SaltRawMaterial"@en ;
                rdfs:seeAlso "https://en.wikipedia.org/wiki/Salt#Salt_in_food"@en ,
                             "https://it.wikipedia.org/wiki/Sale_da_cucina#Usi"@it .


###  http://aims.fao.org/aos/agrovoc/c_4826
agrovoc:c_4826 rdf:type owl:Class ;
               rdfs:subClassOf obo:UBERON_0001913 ,
                               food-upper:MateriaPrima ;
               rdfs:comment "A milk used as a raw material in the cheese production process."@en ,
                            "Un latte usato come materia prima nella produzione di un formaggio."@it ;
               rdfs:label "MateriaPrimaLatte"@it ,
                          "MilkRawMaterial"@en .


###  http://aims.fao.org/aos/agrovoc/c_8309
agrovoc:c_8309 rdf:type owl:Class ;
               rdfs:subClassOf obo:FOODON_00002340 ,
                               food-upper:MateriaPrima ;
               rdfs:comment "A kind of water when used as a raw material in the cheese production process."@en ,
                            "Un tipo di acqua quando utilizzata come materia prima nel processo di produzione del formaggio."@it ;
               rdfs:label "MateriaPrimaAcqua"@it ,
                          "WaterRawMaterial"@en .


###  http://aims.fao.org/aos/agrovoc/c_8376
agrovoc:c_8376 rdf:type owl:Class ;
               rdfs:subClassOf food-upper:MateriaPrima ;
               rdfs:comment "Il siero di latte è la parte liquida del latte che si separa dalla cagliata durante la caseificazione."@it ,
                            "Whey is the liquid remaining after milk has been curdled and strained in the cheesemaking process."@en ;
               rdfs:label "Siero"@it ,
                          "Whey"@en ;
               rdfs:seeAlso "https://en.wikipedia.org/wiki/Whey"@en ,
                            "https://it.wikipedia.org/wiki/Siero_di_latte"@it .


###  http://dbpedia.org/ontology/PopulatedPlace
dbo:PopulatedPlace rdf:type owl:Class ;
                   rdfs:comment "A place or area with clustered or scattered buildings and a permanent human population referenced with geographic coordinates."@en ,
                                "Un luogo o un area dotata di edifici raggruppati o sparsi e una popolazione umana a cui è possibile fare riferimento mediante coordinate geografiche."@it ;
                   rdfs:label "InsediamentoUmano"@it ,
                              "PopulatedPlace"@en ;
                   rdfs:seeAlso "http://en.wikipedia.org/wiki/Populated_place"@en ,
                                "https://it.wikipedia.org/wiki/Insediamento_umano"@it .


###  http://purl.obolibrary.org/obo/ENVO_00000067
obo:ENVO_00000067 rdf:type owl:Class ;
                  rdfs:subClassOf :Environment ;
                  rdfs:comment "A large hole in the side of a hill, cliff, or mountain, or one that is underground, used for cheese affinage."@en ,
                               "Cavità naturale a sviluppo prevalentemente orizzontale che si addentra nei fianchi di un monte, di una collina, di una scarpata o nel sottosuolo, utilizzata per l'affinatura dei formaggi."@it ;
                  rdfs:label "Cave"@en ,
                             "Grotta"@it .


###  http://purl.obolibrary.org/obo/ENVO_01000583
obo:ENVO_01000583 rdf:type owl:Class ;
                  rdfs:subClassOf :Environment ;
                  rdfs:comment "A large piece of equipment that uses electricity to preserve cheese at a cold temperature."@en ,
                               "Un locale di conservazione del formaggio, ove viene mantenuta una temperatura inferiore a quella ambientale."@it ;
                  rdfs:label "CellaFrigorifera"@it ,
                             "Refrigerator"@en .


###  http://purl.obolibrary.org/obo/ENVO_01001871
obo:ENVO_01001871 rdf:type owl:Class ;
                  rdfs:subClassOf :Environment ;
                  rdfs:comment "A large hole in the ground, varying in depth, used for cheese affinage."@en ,
                               "Scavatura del terreno, più o meno profonda, praticata per l'affinatura dei formaggi."@it ;
                  rdfs:label "Fossa"@it ,
                             "Pit"@en .


###  http://purl.obolibrary.org/obo/FOODON_00001013
obo:FOODON_00001013 rdf:type owl:Class ;
                    rdfs:subClassOf obo:FOODON_00002403 ;
                    owl:disjointUnionOf ( obo:FOODON_00001124
                                          obo:FOODON_00001125
                                          obo:FOODON_00001126
                                          :BuffaloCheese
                                          :MixedMilkCheese
                                        ) ,
                                        ( obo:FOODON_00001945
                                          obo:FOODON_00002230
                                          obo:FOODON_00002231
                                          obo:FOODON_00002252
                                          :MediumHardCheese
                                        ) ,
                                        ( obo:FOODON_03401217
                                          :SkimmedMilkCheese
                                          :WholeMilkCheese
                                        ) ;
                    rdfs:comment "Cheese is a dairy product produced in wide ranges of flavors, textures, and forms by coagulation of the milk protein casein."@en ,
                                 "Il formaggio è un prodotto caseario ottenuto dalla coagulazione acida o presamica del latte intero, parzialmente o totalmente scremato per coagulazione delle caseine, facendo anche uso di fermenti e sale da cucina."@it ;
                    rdfs:label "Cheese"@en ,
                               "Formaggio"@it ;
                    rdfs:seeAlso "https://en.wikipedia.org/wiki/Cheese"@en ,
                                 "https://it.wikipedia.org/wiki/Formaggio"@it .


###  http://purl.obolibrary.org/obo/FOODON_00001065
obo:FOODON_00001065 rdf:type owl:Class ;
                    rdfs:subClassOf :MoldRipenedCheese ;
                    rdfs:comment "Cheese which production process entails the appearance of blue-green stripes and spots in the core, caused by the fungi cultures to which the forms are subjected."@en ,
                                 "Un formaggio il cui processo di lavorazione prevede la comparsa di striature e chiazze verdi-blu all'interno della pasta, dovute alle colture di funghi alle quali le forme vengono sottoposte."@it ;
                    rdfs:label "BlueCheese"@en ,
                               "FormaggioErborinato"@it ;
                    rdfs:seeAlso "https://en.wikipedia.org/wiki/Blue_cheese"@en ,
                                 "https://it.wikipedia.org/wiki/Classificazione_dei_formaggi#Formaggi_a_pasta_erborinata"@it .


###  http://purl.obolibrary.org/obo/FOODON_00001108
obo:FOODON_00001108 rdf:type owl:Class ;
                    rdfs:subClassOf obo:UBERON_0001913 ;
                    rdfs:comment "Milk produced by milking buffalos."@en ,
                                 "Un latte prodotto dalla mungitura dei bufali."@it ;
                    rdfs:label "BuffaloMilk"@en ,
                               "LatteBufalino"@it ;
                    rdfs:seeAlso "https://en.wikipedia.org/wiki/Buffalo_milk"@en ,
                                 "https://it.wikipedia.org/wiki/Latte_bufalino"@it .


###  http://purl.obolibrary.org/obo/FOODON_00001112
obo:FOODON_00001112 rdf:type owl:Class ;
                    rdfs:subClassOf obo:UBERON_0001913 ;
                    rdfs:comment "Latte prodotto dalla mungitura delle pecore."@it ,
                                 "Milk produced by milking sheeps."@en ;
                    rdfs:label "LattePecorino"@it ,
                               "SheepMilk"@en ;
                    rdfs:seeAlso "https://en.wikipedia.org/wiki/Sheep_milk"@it ,
                                 "https://it.wikipedia.org/wiki/Latte_pecorino"@it .


###  http://purl.obolibrary.org/obo/FOODON_00001124
obo:FOODON_00001124 rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( obo:FOODON_00001013
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :isMadeWithMilk ;
                                                                 owl:allValuesFrom obo:FOODON_00001911
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:comment "Cheese made with goat milk."@en ,
                                 "Un formaggio prodotto da latte di capra."@it ;
